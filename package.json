{
  "name": "cra-ts",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "date-fns": "1.29.0",
    "emotion": "9.2.6",
    "formik": "1.0.2",
    "husky": "1.3.1",
    "intl": "1.2.5",
    "lodash": "4.17.10",
    "mobx": "5.0.3",
    "mobx-react": "5.2.3",
    "mobx-react-router": "4.0.4",
    "modern-normalize": "0.5.0",
    "react": "16.4.1",
    "react-dom": "16.4.1",
    "react-emotion": "9.2.6",
    "react-helmet": "5.2.0",
    "react-intl": "2.4.0",
    "react-loadable": "5.4.0",
    "react-router": "4.3.1",
    "react-router-dom": "4.3.1",
    "react-scripts-ts": "2.16.0",
    "sentry": "0.1.2",
    "source-map-explorer": "1.5.0"
  },
  "scripts": {
    "start": "react-scripts-ts start",
    "build": "react-scripts-ts build",
    "test": "react-scripts-ts test --env=jsdom",
    "eject": "react-scripts-ts eject",
    "types-installer": "types-installer",
    "analyze": "source-map-explorer",
    "build-analyze": "yarn build && yarn analyze"
  },
  "devDependencies": {
    "@types/date-fns": "2.6.0",
    "@types/intl": "1.2.0",
    "@types/jest": "23.3.0",
    "@types/lodash": "4.14.113",
    "@types/node": "10.5.3",
    "@types/react": "16.4.7",
    "@types/react-dom": "16.0.6",
    "@types/react-helmet": "5.0.6",
    "@types/react-intl": "2.3.8",
    "@types/react-loadable": "5.4.0",
    "@types/react-router": "4.0.29",
    "@types/react-router-dom": "4.3.0",
    "@types/typescript": "2.0.0",
    "lint-staged": "8.1.5",
    "mobx-react-devtools": "6.0.1",
    "prettier": "1.16.4",
    "types-installer": "1.4.0",
    "typescript": "2.9.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts, tsx}": [
      "prettier --write",
      "tslint --fix",
      "git add"
    ],
    "*.{js,jsx}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ],
    "*.{json,css,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
